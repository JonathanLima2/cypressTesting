{
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["cypress"]
    },
    "include": [
        "cypress/**/*.ts",                      
        "cypress/support/**/*.ts",     
    ],
    "exclude": [
        "node_modules"
    ]
}